{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:GetBucketVersioning",
          "s3:PutBucketVersioning",
          "s3:ReplicateObject",
          "s3:ReplicateDelete",
          "s3:ReplicateTags",
          "s3:GetObjectVersionTagging"
        ],
        "Resource": [
          "arn:aws:s3:::target-bucket",
          "arn:aws:s3:::target-bucket/*"
        ]
      }
    ]
  }